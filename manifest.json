{
  "manifest_version": 2,
  "name": "YouTube translator",
  "description": "",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://youtube.com/watch*", "*://www.youtube.com/watch*"],
      "js": ["script.js", "responsivevoice.js", "xml_extractor.js", "player.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Click here!"
  }
}
